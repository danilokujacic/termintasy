{
  "name": "termintasy",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/termintasy/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "serviceWorker": true,
        "ngswConfigPath": "apps/termintasy/src/ngsw-config.json",
        "outputPath": "dist/apps/termintasy",
        "index": "apps/termintasy/src/index.html",
        "browser": "apps/termintasy/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/termintasy/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/termintasy/public"
          },
          "apps/termintasy/src/manifest.webmanifest",
          "apps/termintasy/src/manifest.webmanifest"
        ],
        "styles": [
          "apps/termintasy/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": "apps/termintasy/ngsw-config.json"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "termintasy:build:production"
        },
        "development": {
          "buildTarget": "termintasy:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "termintasy:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/termintasy/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "termintasy:build",
        "staticFilePath": "dist/apps/termintasy/browser",
        "spa": true
      }
    }
  }
}